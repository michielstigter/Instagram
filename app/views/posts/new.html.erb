<html>
  <head>
    <title>New Post</title>
  </head>
  <body>
    <%= form_for @post do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %>
      <%= f.label :picture %>
      <%= f.file_field :picture %>
      <%= f.label :tag_list, "Tags" %>
      <%= f.text_field :tag_list %>
      <%= f.submit 'Create Post' %>
    <% end %>
  </body>
</html>
